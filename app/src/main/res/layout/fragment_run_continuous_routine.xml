<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:fitsSystemWindows="true"
    android:transitionName="editCRoutineRoot"
    tools:context=".EditContinuousRoutineFragment">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_RunRoutine_continuous_routine_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="30sp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline5"
        android:transitionName="editCRoutineName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/str_tv_RunRoutine_continuous_routineName" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_0"
        android:layout_marginVertical="20dp"
        android:layout_width="180dp"
        android:layout_height="0sp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toTopOf="@id/tile_RunCon_2"
        app:layout_constraintEnd_toStartOf="@id/tile_RunCon_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_RunRoutine_continuous_routine_name" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_1"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toBottomOf="@+id/tile_RunCon_0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tile_RunCon_0"
        app:layout_constraintTop_toTopOf="@id/tile_RunCon_0" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_2"
        android:layout_marginVertical="20dp"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toTopOf="@id/tile_RunCon_4"
        app:layout_constraintEnd_toStartOf="@id/tile_RunCon_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tile_RunCon_0" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_3"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toBottomOf="@+id/tile_RunCon_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tile_RunCon_2"
        app:layout_constraintTop_toTopOf="@+id/tile_RunCon_2" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_4"
        android:layout_marginVertical="20dp"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toTopOf="@id/tile_RunCon_6"
        app:layout_constraintEnd_toStartOf="@id/tile_RunCon_5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tile_RunCon_2" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_5"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toBottomOf="@+id/tile_RunCon_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tile_RunCon_4"
        app:layout_constraintTop_toTopOf="@+id/tile_RunCon_4" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_6"
        android:layout_marginVertical="20dp"
        android:layout_width="180dp"
        android:layout_height="0sp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tile_RunCon_7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tile_RunCon_4" />

    <de.threateningcodecomments.views.ContinuousTile
        android:id="@+id/tile_RunCon_7"
        android:layout_width="180dp"
        android:layout_height="0sp"
        app:layout_constraintBottom_toBottomOf="@+id/tile_RunCon_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tile_RunCon_6"
        app:layout_constraintTop_toTopOf="@+id/tile_RunCon_6" />


</androidx.constraintlayout.widget.ConstraintLayout>